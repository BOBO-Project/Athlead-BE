{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/auth",
      "dest": "/app.js",
      "methods": ["POST"]
    },
    {
      "src": "/email",
      "dest": "/app.js",
      "methods": ["POST", "GET"]
    },
    {
      "src": "/partners",
      "dest": "/app.js",
      "methods": ["POST", "GET", "PUT", "DELETE"]
    }
  ]
}
